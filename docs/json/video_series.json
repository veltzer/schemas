{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "movies that I saw",
	"type": "object",
	"properties": {
		"$schema": {
			"type": "string",
			"format": "uri"
		},
		"items": {
			"type": "array",
			"items": {
				"title": "series_saw",
				"type": "object",
				"properties": {
					"name": {
						"description": "Name of the movie",
						"type": "string"
					},
					"device": {
						"description": "On what device did I see the movie",
						"type": "string"
					},
					"date": {
						"description": "On what date did I see the movie (must be in UTCZ format)",
						"type": "string",
						"format": "date-time",
						"pattern": "Z$"
					},
					"timezone": {
						"description": "The IANA timezone name for the date. Must be one of the specified values.",
						"type": "string",
						"enum": [
							"Asia/Jerusalem",
							"America/New_York",
							"America/Los_Angeles",
							"Europe/London",
							"Europe/Paris",
							"Asia/Tokyo",
							"Australia/Sydney",
							"UTC"
						]
					},
					"imdb_id": {
						"description": "The imdb id of the movie",
						"type": "string"
					},
					"location": {
						"description": "Where did I see the movie",
						"type": "string"
					},
					"seasons_seen": {
						"description": "What seasons did I see",
						"type": "array",
						"items": {
							"type": "integer"
						}
					},
					"seasons_not_seen": {
						"description": "What seasons didnt I see",
						"type": "array",
						"items": {
							"type": "integer"
						}
					},
					"rating": {
						"description": "What rating did I give the movie",
						"type": "integer"
					},
					"review": {
						"description": "What review did I give the movie",
						"type": "string"
					},
					"with": {
						"description": "Who did I see it with",
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				},
				"required": ["name", "imdb_id", "location", "device", "rating", "review","date","timezone"],
				"additionalProperties": false
			}
		}
	},
	"required": ["items"],
	"additionalProperties": false
}
