{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "common.json",
	"description": "Common elements used all over, put stuff here before it gets it's own file",
	"$defs": {
		"$schema": {
			"type": "string",
			"format": "uri"
		},
		"rating": {
			"description": "A ratings from 1 to 10",
			"type": "integer",
			"minimum": 1,
			"maximum": 10
		},
		"tickets": {
			"description": "How many tickets do I have",
			"type": "integer",
			"minimum": 1
		},
		"url": {
			"description": "A valid URL",
			"type": "string",
			"format": "uri"
		},
		"duration": {
			"description": "Duration of something",
			"type": "string"
		},
		"review": {
			"description": "What review did I give the movie",
			"type": "string"
		},
		"name": {
			"description": "Name of the movie",
			"type": "string"
		},
		"description": {
			"description": "Description of something",
			"type": "string"
		},
		"remark": {
			"description": "Remark about something",
			"type": "string"
		},
		"imdb_id": {
			"description": "The imdb id of the movie",
			"type": "string"
		},
		"id": {
			"description": "id of the video",
			"type": "string"
		},
		"seasons_seen": {
			"description": "What seasons did I see",
			"type": "array",
			"items": {
				"type": "integer",
				"minimum": 1
			}
		},
		"seasons_not_seen": {
			"description": "What seasons didnt I see",
			"type": "array",
			"items": {
				"type": "integer",
				"minimum": 1
			}
		}
	}
}
