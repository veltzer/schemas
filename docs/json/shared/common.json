{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$defs": {
		"$schema": {
			"type": "string",
			"format": "uri"
		},
		"rating": {
			"type": "integer",
			"minimum": 1,
			"maximum": 10
		},
		"date_utcz": {
			"description": "On what date did I see the movie (must be in UTCZ format)",
			"type": "string",
			"format": "date-time",
			"pattern": "Z$"
		},
		"date_my": {
			"description": "A date containing only the year and month, in YYYY-MM format.",
			"type": "string",
			"pattern": "^\\d{4}-\\d{2}$",
			"examples": [
				"2023-10",
				"1999-05"
			]
		},
		"date_y": {
			"description": "A date containing only the year, in YYYY format.",
			"type": "string",
			"pattern": "^\\d{4}$",
			"examples": [
				"2023",
				"1999"
			]
		},
		"date_timezone": {
			"description": "The IANA timezone name for the date. Must be one of the specified values.",
			"type": "string",
			"enum": [
				"Asia/Jerusalem",
				"America/New_York",
				"America/Los_Angeles",
				"Europe/London",
				"Europe/Paris",
				"Asia/Tokyo",
				"Australia/Sydney",
				"UTC"
			]
		},
		"device": {
			"description": "On what device did I see the movie",
			"type": "string"
		},
		"review": {
			"description": "What review did I give the movie",
			"type": "string"
		},
		"with": {
			"description": "Who did I see it with",
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"lecturers": {
			"description": "List of lecturers",
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"name": {
			"description": "Name of the movie",
			"type": "string"
		},
		"description": {
			"description": "Description of something",
			"type": "string"
		},
		"location": {
			"description": "Where did I see the movie",
			"type": "string"
		},
		"imdb_id": {
			"description": "The imdb id of the movie",
			"type": "string"
		},
		"id": {
			"description": "id of the video",
			"type": "string"
		},
		"seasons_seen": {
			"description": "What seasons did I see",
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"seasons_not_seen": {
			"description": "What seasons didnt I see",
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"course_provider": {
			"description": "provider of the course",
			"type": "string",
			"enum": [
				"Coursera",
				"MySQLAB",
				"AWS",
				"Sun Microsystems"
			]
		}
	}
}
