{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "device.json",
	"description": "Device element",
	"$defs": {
		"device": {
			"description": "A device",
			"type": "string",
			"enum": [
				"Device at location",

				"Netflix",
				"Partner TV",
				"Prime TV",
				"Cellcom TV",
				"YES",

				"Samsung S10",
				"Samsung S21",
				"Samsung S7",
				"Sansa Clip 2",
				"Home TV",
				"Desktop",
				"Laptop"
			]
		},
		"devices": {
			"description": "A collection of devices",
			"type": "array",
			"items": {
				"$ref": "#/$defs/device"
			}
		}
	}
}
