{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "device.json",
	"description": "Device element",
	"$defs": {
		"device": {
			"description": "A device",
			"type": "string",
			"enum": [
				"cellcom_tv",
				"desktop",
				"desktop computer",
				"device at location",
				"harddrive",
				"home tv",
				"laptop",
				"Laptop from twiggle",
				"netflix",
				"partner TV",
				"Partner VOD",
				"Prime",
				"PrimeTV",
				"primevideo",
				"Samsung S10",
				"Samsung S21",
				"Samsung S7",
				"Sansa Clip 2",
				"YES",
				"YES VOD"
			]
		},
		"devices": {
			"description": "A collection of devices",
			"type": "array",
			"items": {
				"$ref": "#/$defs/device"
			}
		}
	}
}
