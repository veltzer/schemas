{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "location.json",
	"description": "Provider of courses",
	"$defs": {
		"location": {
			"description": "A location",
			"type": "string",
			"enum": [
				"Home",
				"Seminar Hakibutzim",
				"Michlelet Tel-Aviv Yafo",
				"Beit Hakibutz Hameuhad, Leonardo Da Vinci, Tel-Aviv",
				"TalkHouse in TelAviv port",
				"Twiggle offices at Even-Gvirol 2"
