{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "text_books.json",
	"decription": "Books that I read",
	"type": "object",
	"properties": {
		"$schema": {
			"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/$schema"
		},
		"items": {
			"type": "array",
			"items": {
				"title": "...",
				"description": "...",
				"type": "object",
				"properties": {
					"id": {
						"description": "The unique identifier for the read",
						"type": "number"
					},
					"url": {
						"description": "url of the article",
						"type": "string"
					},
					"file": {
						"description": "file of the article",
						"type": "string"
					},
					"location": {
						"description": "Where did the reading take place",
						"type": "string"
					},
					"date_utcz": {
						"description": "When did the reading take place",
						"type": "string"
					}
				}
			},
			"required": [
				"name",
				"authors",
				"rating",
				"review"
			],
			"additionalProperties": false
		}
	},
	"required": ["items"],
	"additionalProperties": false
}
