{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "text_books.json",
	"decription": "Books that I read",
	"type" : "array",
	"items" : {
		"title" : "...",
		"description" : "...",
		"type" : "object",
		"properties" : {
			"who": {
				"type": "array",
				"items": {
					"type": "string"
				},
				"minItems": 1,
				"uniqueItems": true
			},
			"location" : {
				"description": "Where did the reading take place",
				"type" : "string"
			},
			"date" : {
				"description": "When did the reading take place",
				"type" : "string"
			}
		},
		"required": [
			"who",
			"location",
			"date"
		]
	}
}
