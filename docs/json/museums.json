				"properties": {
					"name": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/name"
					},
					"city": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/city.json#/$defs/city"
