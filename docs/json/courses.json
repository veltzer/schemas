{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "courses.json",
	"description": "Courses that I took",
	"type": "object",
	"properties": {
		"$schema": {
			"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/$schema"
		},
		"items": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/name"
					},
					"course_provider": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/course_provider"
					}
				},
				"required": ["name", "course_provider" ],
				"additionalProperties": false
			}
		}
	},
	"required": ["items"],
	"additionalProperties": false
}
