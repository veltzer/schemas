{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "courses that I took",
	"type": "object",
	"properties": {
		"$schema": {
			"type": "string",
			"format": "uri"
		},
		"items": {
			"type": "array",
			"items": {
				"title": "course taken",
				"type": "object",
				"properties": {
					"name": {
						"description": "Name of the course",
						"type": "string"
					},
					"provider": {
						"description": "provider of the course",
						"type": "string"
					}
				},
				"required": ["name", "provider" ],
				"additionalProperties": false
			}
		}
	},
	"required": ["items"],
	"additionalProperties": false
}
