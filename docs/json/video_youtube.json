{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "youtube videos that I saw",
	"type": "object",
	"properties": {
		"$schema": {
			"type": "string",
			"format": "uri"
		},
		"items": {
			"type": "array",
			"items": {
				"title": "youtube video seen",
				"type": "object",
				"properties": {
					"name": {
						"description": "Name of the video",
						"type": "string"
					},
					"id": {
						"description": "id of the video",
						"type": "string"
					},
					"date": {
						"description": "On what date did I see the movie (must be in UTCZ format)",
						"type": "string",
						"format": "date-time",
						"pattern": "Z$"
					},
					"timezone": {
						"description": "The IANA timezone name for the date. Must be one of the specified values.",
						"type": "string",
						"enum": [
							"Asia/Jerusalem",
							"America/New_York",
							"America/Los_Angeles",
							"Europe/London",
							"Europe/Paris",
							"Asia/Tokyo",
							"Australia/Sydney",
							"UTC"
						]
					},
					"device": {
						"description": "On what device",
						"type": "string"
					},
					"location": {
						"description": "Where did I see the movie",
						"type": "string"
					},
					"rating": {
						"description": "What rating did I give the movie",
						"type": "integer"
					},
					"review": {
						"description": "What review did I give the movie",
						"type": "string"
					}
				},
				"required": ["id", "date", "timezone", "review", "rating"],
				"additionalProperties": false
			}
		}
	},
	"required": ["items"],
	"additionalProperties": false
}
