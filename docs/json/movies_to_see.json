{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "movies_to_see.json",
	"description": "Movies to see",
	"type": "object",
	"properties": {
		"$schema": {
			"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/$schema"
		},
		"items" : {
			"title" : "...",
			"description" : "...",
			"type" : "object",
			"properties" : {
				"recommended_by": {
					"description": "Who recommended the movie",
					"type": "string"
				},
				"imdb_id" : {
					"description": "The imdb id of the movie",
					"type" : "string"
				},
				"genre" : {
					"description": "What is the genre of the movie",
					"type" : "string"
				}
			},
			"required": ["recommended_by", "imdb_id", "genre"]
		}
	}
}
