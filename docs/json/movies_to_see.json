{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "movies to see",
	"type" : "array",
	"items" : {
		"title" : "movie_to_see",
		"type" : "object",
		"properties" : {
			"recommended_by": {
				"description": "Who recommended the movie",
				"type": "string"
			},
			"imdb_id" : {
				"description": "The imdb id of the movie",
				"type" : "string"
			},
			"genre" : {
				"description": "What is the genre of the movie",
				"type" : "string"
			}
		},
		"required": ["recommended_by", "imdb_id", "genre"]
	}
}
