{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "trips.json",
	"description": "Trips that I have made",
	"type": "object",
	"properties": {
		"$schema": {
			"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/$schema"
		},
		"items": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"where": {
						"description": "where did I go",
						"type": "string"
					},
					"countries": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/country.json#/$defs/countries"
					},
					"date_utcz": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_utcz"
					},
					"date_y": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_y"
					},
					"date_my": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_my"
					},
					"date_dmy": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_dmy"
					},
					"date_timezone": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_utcz_timezone"
					},
					"duration_days": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/duration.json#/$defs/duration_days"
					},
					"remark": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/remark"
					},
					"with": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/person.json#/$defs/with"
					}
				},
				"required": [
					"where",
					"countries",
					"date_timezone",
					"duration_days",
					"with"
				],
				"additionalProperties": false
			}
		}
	},
	"required": ["items"],
	"additionalProperties": false
}
