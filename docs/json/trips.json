{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "trips.json",
	"description": "Trips that I have made",
	"type": "object",
	"properties": {
		"$schema": {
			"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/$schema"
		},
		"items": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"location": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/location.json#/$defs/location"
					},
					"country": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/country.json#/$defs/country"
					},
					"countries": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/country.json#/$defs/countries"
					},
					"date_utcz": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_utcz"
					},
					"date_y": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_y"
					},
					"date_my": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_my"
					},
					"date_dmy": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_dmy"
					},
					"date_utcz_timezone": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_utcz_timezone"
					},
					"date_y_timezone": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_y_timezone"
					},
					"date_my_timezone": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_my_timezone"
					},
					"date_dmy_timezone": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_dmy_timezone"
					},
					"with": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/person.json#/$defs/with"
					},
					"remark": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/remark"
					},
					"duration": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/duration"
					}
				},
				"required": [
					"location",
					"with"
				],
				"additionalProperties": false
			}
		}
	},
	"required": ["items"],
	"additionalProperties": false
}
