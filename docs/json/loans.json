{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "loans.json",
	"description": "Stuff that I loaned",
	"type": "object",
	"properties": {
		"$schema": {
			"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/$schema"
		},
		"items": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"person": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/country.json#/$defs/person"
					},
					"item": {
						"description": "What item was loaned",
						"type": "string"
					},
					"date_utcz": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_utcz"
					},
					"date_timezone": {
						"$ref": "https://veltzer.github.io/schemas/json/shared/common.json#/$defs/date_timezone"
					}
				},
				"required": [
					"person",
					"item",
					"date_utcz",
					"date_timezone"
				],
				"additionalProperties": false
			}
		}
	},
	"required": ["items"],
        "additionalProperties": false
}
